import java.util.Scanner;

public class CoordinateQuadrant {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("Inserisci una coordinata: ");
        double x = scanner.nextDouble();
        
        System.out.print("Inserisci un'altra coordinata: ");
        double y = scanner.nextDouble();
        
        if (x > 0 && y > 0) {
            System.out.println("primo quadrante");
        } else if (x < 0 && y > 0) {
            System.out.println("secondo quadrante");
        } else if (x < 0 && y < 0) {
            System.out.println("terzo quadrante");
        } else if (x > 0 && y < 0) {
            System.out.println("quarto quadrante");
        }
        
        scanner.close();
    }
}

